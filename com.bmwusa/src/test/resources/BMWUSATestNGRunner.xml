<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="BMWUSA" verbose="1" >

    <listeners>
        <listener class-name="listeners.AnnotationTransformer"/>
    </listeners>

    <parameter name="browser" value="chrome"/>
    <parameter name="url" value="http://bmwusa.com"/>
    <test name="Test">
        <classes>
            <class name="TestFiles.TestChoosingModels"></class>
            <class name="TestFiles.TestContactDealer"></class>
            <class name="TestFiles.TestCustomizeCar"></class>
            <class name="TestFiles.TestEstimatePayment"></class>
            <class name="TestFiles.TestGetQuote"></class>
            <class name="TestFiles.TestNavigateToDealerContactPage"></class>
            <class name="TestFiles.TestRegistration"></class>
            <class name="TestFiles.TestShopNewVehicle"></class>
            <class name="TestFiles.TestShopBmwAccessories"></class>
            <class name="TestFiles.TestNavigateToTheBuildingPage"></class>

        </classes>
    </test>

</suite>
